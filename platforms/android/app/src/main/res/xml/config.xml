<?xml version='1.0' encoding='utf-8'?>
<widget id="com.msap.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Bliimo Conference App</name>
    <description>
        Bliimo conference app
    </description>
    <author email="info@bliimo.com" href="http://bliimo.com">
        Bliimo Technologies Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="assets/mdpi.png" />
    <icon density="ldpi" src="assets/ldpi.png" />
    <icon density="hdpi" src="assets/hdpi.png" />
    <icon density="xhdpi" src="assets/xdpi.png" />
    <icon density="xxhdpi" src="assets/xxdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
